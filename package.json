{
  "name": "mturk",
  "private": true,
  "version": "0.0.0",
  "description": "a Sails application",
  "keywords": [],
  "dependencies": {
    "sails": "^1.0.0-35",
    "grunt": "1.0.1",
    "sails-hook-orm": "^2.0.0-16",
    "sails-hook-sockets": "^1.4.0",
    "sails-hook-grunt": "^2.0.0",
    "lodash": "3.10.1",
    "async": "2.0.1"
  },
  "devDependencies": {
    "eslint": "3.5.0"
  },
  "scripts": {
    "start": "concurrently --kill-others 'npm run watch:frontend' 'npm run watch:backend'",
    "watch:frontend": "ng build -w",
    "watch:backend": "nodemon app.js",
    "frontend": "ng serve",
    "backend": "ng build && nodemon app/app.js",
    "build": "ng build -prod",
    "ng": "ng",
    "e2e": "ng e2e",
    "prod": "NODE_ENV=production node app.js",
    "test": "npm run lint && npm run custom-tests && echo 'Done.'",
    "lint": "node ./node_modules/eslint/bin/eslint . --max-warnings=0 && echo 'âœ”  Your code looks good.'",
    "custom-tests": "echo \"(No other custom tests yet.)\" && echo",
    "debug": "node debug app.js"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/iGor/test-project.git"
  },
  "author": "iGor",
  "license": ""
}
